<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundedImageView">
        <attr name="round_radius" format="dimension" />
        <attr name="stroke_width" format="dimension"/>
        <attr name="shape_mode" format="enum">
            <enum name="round_rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
        <attr name="stroke_color" format="color" />
    </declare-styleable>

    <declare-styleable name="RadarView">
        <attr name="maxValue" format="float" />
        <attr name="mainPaintColor" format="color" />
        <attr name="textPaintColor" format="color" />
        <attr name="valuePaintColor" format="color" />
    </declare-styleable>

    <declare-styleable name="FlipBallView">
        <attr name="maxRadius" format="dimension" />
        <attr name="minRadius" format="dimension" />
        <attr name="distance" format="dimension" />
    </declare-styleable>

</resources>